{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/vsp-electronics",
  "installCommand": "npm install",
  "env": {
    "NG_API_URL": "@api_url"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}